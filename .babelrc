{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "entry",
        "corejs": { "version": 3, "proposals": true },
        "targets": {
          "browsers": [
            "last 2 versions",
            "not ie < 11",
            "android >= 4.2"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-custom-element-classes",
    "transform-es2015-classes",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
